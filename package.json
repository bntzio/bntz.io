{
  "name": "bntz.io",
  "author": "Enrique Benitez <bntz.io>",
  "scripts": {
    "start": "node server.js",
    "dev": "next",
    "build": "next build",
    "build:content": "processmd content/**/*.md --outputDir content --summaryOutput content/summary.json",
    "build:content:watch": "processmd content/**/*.md --outputDir content --summaryOutput content/summary.json --watch",
    "build:export": "npm run build && npm run export",
    "export": "next export && cp -R static/* out/",
    "deploy": "npm run build:content && npm run build:export && mv out public",
    "server": "NODE_ENV=production node server.js"
  },
  "dependencies": {
    "gsap": "^1.20.2",
    "next": "^2.4.3",
    "next-redux-wrapper": "^1.3.1",
    "react": "^15.6.0",
    "react-dom": "^15.6.0",
    "react-gsap-enhancer": "^0.3.0",
    "react-redux": "^5.0.5",
    "redux": "^3.7.2",
    "styled-components": "^2.0.1",
    "styled-normalize": "^2.0.0"
  },
  "devDependencies": {
    "babel-plugin-styled-components": "^1.1.4",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.3.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.1.0",
    "eslint-plugin-standard": "^3.0.1",
    "processmd": "^3.1.4",
    "remote-redux-devtools": "^0.5.12"
  }
}
