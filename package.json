{
  "name": "bntz",
  "main": "server.js",
  "now": {
    "alias": "bntz.io",
    "files": [
      "actions",
      "components",
      "content",
      "data",
      "pages",
      "reducers",
      "static",
      "store",
      "utils",
      "routes.js",
      "server.js",
      "yarn.lock"
    ]
  },
  "scripts": {
    "processmd": "processmd content/**/*.md --outputDir static/content --summaryOutput content/summary.json",
    "dev": "node server.js",
    "build": "next build",
    "start": "NODE_ENV=production next start",
    "prod": "npm run build && npm run processmd && now && now alias"
  },
  "dependencies": {
    "express": "^4.15.4",
    "gsap": "^1.20.2",
    "isomorphic-unfetch": "^2.0.0",
    "next": "^3.0.3",
    "next-redux-wrapper": "^1.3.1",
    "next-routes": "^1.0.40",
    "polished": "^1.4.1",
    "react": "^15.6.0",
    "react-dom": "^15.6.0",
    "react-gsap-enhancer": "^0.3.0",
    "react-redux": "^5.0.5",
    "redux": "^3.7.2",
    "styled-components": "^2.0.1",
    "styled-normalize": "^2.0.0",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "babel-plugin-styled-components": "^1.1.4",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.3.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.1.0",
    "eslint-plugin-standard": "^3.0.1",
    "processmd": "^3.1.4",
    "remote-redux-devtools": "^0.5.12"
  }
}
